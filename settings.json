{
	"workbench.iconTheme": "vscode-icons",
	"workbench.colorTheme": "Atom Background Modified",
	"editor.tabSize": 2,
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"security.workspace.trust.untrustedFiles": "open",
	"explorer.confirmDelete": false,
	"liveSassCompile.settings.generateMap": false,
	"liveSassCompile.settings.formats": [
		{
			"format": "expanded",
			"autoprefix": "last 5 versions",
			"extensionName": ".css",
			"savePath": "/css"
		}
	],
	"editor.tokenColorCustomizations": {
		"[One Dark Pro Darker]": {
			"textMateRules": [
				{
					"scope": "constant.numeric",
					"settings": {
						"foreground": "#fff"
					}
				},
				{
					"scope": "support.constant.property-value.css",
					"settings": {
						"foreground": "#fff"
					}
				},
				{
					"scope": "keyword.other.unit",
					"settings": {
						"foreground": "#fff"
					}
				},
				{
					"scope": "entity.other.attribute-name.class.css",
					"settings": {
						"foreground": "#5bd0ff"
					}
				},
				{
					"scope": "punctuation.definition.constant",
					"settings": {
						"foreground": "#fff"
					}
				},
				{
					"scope": "constant",
					"settings": {
						"foreground": "#fff"
					}
				},
				{
					"scope": "entity.name.tag",
					"settings": {
						"foreground": "#ff5e73"
					}
				}
			]
		}
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[scss]": {
		// "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[css]": {
		"editor.defaultFormatter": "HookyQR.beautify"
	},
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"ecsstractor_port.bem_nesting": true,
	"editor.wordWrap": "on",
	"editor.formatOnSave": false,
	"editor.unicodeHighlight.allowedCharacters": {
		"ะต": true
	},
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"workbench.startupEditor": "newUntitledFile",
	"editor.smoothScrolling": true,
	"editor.minimap.renderCharacters": false,
	"notebook.breadcrumbs.showCodeCells": false,
	"files.defaultLanguage": "html",
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"editor.renderWhitespace": "boundary",
	"editor.lineHeight": 23,
	"security.workspace.trust.enabled": false,
	"editor.insertSpaces": false,
	"editor.linkedEditing": true,
	"ecsstractor_port.add_comment": false,
	"ecsstractor_port.empty_line_before_nested_selector": false,
	"liveServer.settings.donotVerifyTags": true,
	"prettier.bracketSameLine": true,
	"prettier.printWidth": 200,
	"workbench.layoutControl.enabled": false,
	"[dart]": {
		"editor.formatOnSave": true,
		"editor.formatOnType": true,
		"editor.rulers": [80],
		"editor.selectionHighlight": false,
		"editor.suggest.snippetsPreventQuickSuggestions": false,
		"editor.suggestSelection": "first",
		"editor.tabCompletion": "onlySnippets",
		"editor.wordBasedSuggestions": "off"
	},
	"terminal.integrated.fontFamily": "JetBrains mono",
	"terminal.integrated.shellArgs.linux": ["-l"],
	"liveSassCompile.settings.includeItems": [],
	"vsicons.dontShowNewVersionMessage": true,
	"gitlens.advanced.messages": {
		"suppressLineUncommittedWarning": true
	},
	"tabnine.experimentalAutoImports": true,
	"cssPeek.peekFromLanguages": [
		"html",
		"django-html",
		"laravel-blade",
		"razor",
		"vue",
		"blade",
		"pug",
		"jade",
		"handlebars",
		"php",
		"twig",
		"md",
		"nunjucks",
		"javascript",
		"javascriptreact",
		"erb",
		"typescript",
		"typescriptreact",
		"HTML (Eex)",
		"html-eex",
		"ejs",
		"hbs",
		"svelte",
		"tsx"
	],
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"gitlens.ai.experimental.provider": "openai",
	"gitlens.ai.experimental.openai.model": "gpt-4-1106-preview",
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.cursorSmoothCaretAnimation": "on",
	"redhat.telemetry.enabled": false,
	"prettier.useTabs": true,
	"prettier.useEditorConfig": false,
	"prettier.tabWidth": 2,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
	"explorer.compactFolders": false,
	"yaml.schemas": {
		"file:///home/ioan/.vscode/extensions/atlassian.atlascode-3.0.10/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
	},
	"atlascode.jira.jqlList": [
		{
			"id": "ab7e79a4-4117-4be1-b1e9-d394fa0e1a3b",
			"enabled": true,
			"name": "My revo-factory Issues",
			"query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
			"siteId": "0786d892-49d2-490d-9ae3-4ccc4429511d",
			"monitor": true
		}
	],
	"typescript.updateImportsOnFileMove.enabled": "always",
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"editor.mouseWheelZoom": true,
	"editor.fontFamily": "'Fira Code'",
	"editor.fontLigatures": true,
	"sync.gist": "100898167c935651719b86cf5192b669",
	"prettier.arrowParens": "always",
	"codeium.enableConfig": {
		"*": true,
		"prisma": true
	},
	"prettier.singleQuote": false,
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[ftl]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"cloudcode.project": "revo-duet",
	"cloudcode.duetAI.project": "revo-duet",
	// "sonarlint.connectedMode.project": {
	// 	"connectionId": "http-localhost-9002",
	// 	"projectKey": "ou-app"
	// },
	"sonarlint.focusOnNewCode": false,
	"workbench.editor.editorActionsLocation": "hidden",
	"editor.minimap.enabled": false,
	"prisma.showPrismaDataPlatformNotification": false
}
